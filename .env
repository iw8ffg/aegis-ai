# File .env - Configurazione di Aegis AI

# Credenziali Database PostgreSQL
POSTGRES_DB=aegis_db
POSTGRES_USER=aegis_user
POSTGRES_PASSWORD=una_password_molto_sicura

# Configurazione Server Email (SMTP)
SMTP_SERVER=smtp.tuoprovider.com
SMTP_PORT=587
SMTP_USER=tua_email@dominio.com
SMTP_PASSWORD=tua_password_email

# Altre Chiavi API (es. per servizi meteo, mappe, ecc.)
WEATHER_API_KEY=CHIAVE_API_QUI
MAPBOX_TOKEN=TOKEN_MAPBOX_QUI
